@using MvcOnlineTicariOtomasyon.Models.Siniflar
@model List<KargoTakip>

@{
    ViewBag.Title = "KargoTakip";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Kargo Takip</h2>
<br />

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Açıklama</th>
            <th>Tarih</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var x in Model)
            {
            <tr>
                <td>@x.Aciklama</td>
                <td>@x.TarihZaman</td>
            </tr>
            }
        
    </tbody>

</table>


