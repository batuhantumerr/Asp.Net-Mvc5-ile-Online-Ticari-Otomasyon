@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Cari Paneli</title>
    <link href="~/Web/dist/css/styles.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/js/all.min.js" crossorigin="anonymous"></script>
</head>
<body>
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <a class="navbar-brand" href="/CariPanel/Index">Cari Paneli</a><button class="btn btn-link btn-sm order-1 order-lg-0" id="sidebarToggle" href="#">
            <i class="fas fa-bars"></i>
        </button><!-- Navbar Search-->
        <div class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
            <ul class="navbar-nav ml-auto ml-md-0">

                <li class="nav-item">
                    <a class="nav-link text-white" href="/CariPanel/Index/">Profilim <span class="sr-only">(current)</span></a>
                </li>

                <li class="nav-item">
                    <a class="nav-link  text-white" href="/CariPanel/KargoTakip/">Kargom Nerede</a>
                </li>

                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle  text-white" id="userDropdown" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                        <a class="dropdown-item" href="/CariPanel/Siparislerim/">Siparişlerim</a>
                        <a class="dropdown-item" href="/CariPanel/Yardim/">Yardım</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="/CariPanel/CikisYap/">Çıkış Yap</a>
                    </div>
                </li>
            </ul>
        </div>
        <!-- Navbar-->    
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading">İşlemler</div>
                        <a class="nav-link" href="/CariPanel/Index/">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Profil
                        </a>
                        <a class="nav-link" href="/CariPanel/Siparislerim/">
                            <div class="sb-nav-link-icon"><i class="fas fa-flask"></i></div>
                            Siparişlerim
                        </a>
                        <a class="nav-link" href="/CariPanel/KargoTakip/">
                            <div class="sb-nav-link-icon"><i class="fas fa-bars"></i></div>
                            Kargo Takibi
                        </a>
                        <a class="nav-link" href="/CariPanel/GelenMesajlar/">
                            <div class="sb-nav-link-icon"><i class="fas fa-user"></i></div>
                            Mesajlar
                        </a>
                        @*<a class="nav-link" href="/CariPanel/CikisYap/">
                            <div class="sb-nav-link-icon"><i class="fas fa-street-view"></i></div>
                            Çıkış Yap
                        </a>*@


                    </div>
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </main>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/Web/dist/js/scripts.js"></script>
</body>
</html>
